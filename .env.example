# IntelliRate Setup - Add this to your .env file

# CRITICAL: Get your Groq API key from https://console.groq.com/keys
GROQ_API_KEY=gsk_YOUR_ACTUAL_GROQ_API_KEY_HERE

# Database Configuration (already set up via Docker)
DATABASE_URL=postgresql://postgres:intellirate123@localhost:5432/intellirate

# Redis Configuration
REDIS_URL=redis://localhost:6379

# App Configuration
APP_NAME=IntelliRate Gateway
APP_VERSION=1.0.0
DEBUG=True

# CORS Origins
BACKEND_CORS_ORIGINS=["http://localhost:5173","http://localhost:5174","http://127.0.0.1:5173","http://127.0.0.1:5174"]

# Optional: AWS/Firebase configs
# AWS_ACCESS_KEY_ID=
# AWS_SECRET_ACCESS_KEY=
# FIREBASE_CREDENTIALS_PATH=
